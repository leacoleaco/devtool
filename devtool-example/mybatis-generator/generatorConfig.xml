        </plugin>
        <!-- 枚举列辅助对象生成插件-->
        <plugin type="com.fuyo.mybatis.generator.plugins.EnumTypeStatusPlugin"/>
        <!-- 生成 从 url 构建example方法-->
        <plugin type="com.fuyo.mybatis.generator.plugins.BuildWithUrlParamPlugin"/>

        <commentGenerator type="com.fuyo.mybatis.generator.plugins.comment.CommentGenerator">
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
            <!--<property name="suppressAllComments" value="true"/>-->
        </commentGenerator>

        <!--数据库连接信息-->
        <jdbcConnection driverClass="${jdbc.driver}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.username}"
                        password="${jdbc.password}"/>

        <javaTypeResolver>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.fuyo.cloud.db.biz.vip.g.domain" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="com.fuyo.cloud.db.biz.vip.g.dao" targetProject="src/main/resources"/>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.fuyo.cloud.db.biz.vip.g.dao"
                             targetProject="src/main/java"/>
        <!--表名-->
        <table tableName="biz_user_vip_card" domainObjectName="GVipCardDto">
            <columnOverride column="user_id" javaType="java.lang.Integer"/>
            <columnOverride column="status" javaType="java.lang.Byte"/>
            <columnOverride column="sex" javaType="java.lang.Byte"/>
        </table>
        <table tableName="biz_vip_card_pay" domainObjectName="GVipCardPayDto">
            <columnOverride column="user_id" javaType="java.lang.Integer"/>
        </table>
    </context>

</generatorConfiguration>
